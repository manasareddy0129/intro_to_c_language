#include<stdio.h>
#include<string.h>
int main() {
    char str[100];
    fgets(str, sizeof(str), stdin);
    int freq[256]={0}, i=0;
    while(str[i]!='\0') {
        if(str[i]!='\n') freq[(int)str[i]]++;
        i++;
    }
    for(i=0;i<256;i++) {
        if(freq[i]>0) printf("%c occurs %d times\n", i, freq[i]);
    }
    return 0;
}
