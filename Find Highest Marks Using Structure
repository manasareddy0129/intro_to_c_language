#include<stdio.h>
struct Student { char name[50]; float marks; };
int main() {
    struct Student s[3];
    for(int i=0;i<3;i++) scanf("%s %f", s[i].name,&s[i].marks);
    int index=0;
    for(int i=1;i<3;i++) if(s[i].marks>s[index].marks) index=i;
    printf("%s %.2f", s[index].name, s[index].marks);
    return 0;
}
