#include<stdio.h>
#include<string.h>
int main() {
    char str[100];
    fgets(str, sizeof(str), stdin);
    int i=0, words=0;
    while(str[i]!='\0') {
        if(str[i]==' ' || str[i]=='\n') {
            words++;
            while(str[i]==' ' || str[i]=='\n') i++;
        } else i++;
    }
    if(i>0 && str[i-1]!=' ' && str[i-1]!='\n') words++;
    printf("%d", words);
    return 0;
}
