#include<stdio.h>
int main() {
    char str[100];
    scanf("%s", str);
    int i=0, vowels=0;
    while(str[i]!='\0') {
        char ch=str[i];
        if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||
           ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U') vowels++;
        i++;
    }
    printf("%d", vowels);
    return 0;
}
