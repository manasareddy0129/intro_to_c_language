#include<stdio.h>
#include<string.h>
int main() {
    char str[100];
    scanf("%s", str);
    int len=0, flag=0;
    while(str[len]!='\0') len++;
    for(int i=0;i<len/2;i++) {
        if(str[i]!=str[len-1-i]) { flag=1; break; }
    }
    if(flag) printf("Not Palindrome");
    else printf("Palindrome");
    return 0;
}
