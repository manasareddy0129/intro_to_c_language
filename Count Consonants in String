#include<stdio.h>
int main() {
    char str[100];
    scanf("%s", str);
    int i=0, consonants=0;
    while(str[i]!='\0') {
        char ch=str[i];
        if((ch>='a' && ch<='z') || (ch>='A' && ch<='Z')) {
            if(!(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'||
                 ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U')) consonants++;
        }
        i++;
    }
    printf("%d", consonants);
    return 0;
}
