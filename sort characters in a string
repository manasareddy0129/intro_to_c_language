#include<stdio.h>
#include<string.h>
int main() {
    char str[100], temp;
    fgets(str, sizeof(str), stdin);
    int len=strlen(str)-1;
    for(int i=0;i<len-1;i++) {
        for(int j=i+1;j<len;j++) {
            if(str[i]>str[j]) {
                temp=str[i];
                str[i]=str[j];
                str[j]=temp;
            }
        }
    }
    printf("%s", str);
    return 0;
}
